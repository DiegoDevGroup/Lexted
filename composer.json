{
    "name": "ddg/lexted",
    "description": "Various methods commonly used by the DiegoDev Group on Laravel Projects. Anyone is welcome to use, but you do so under your own risk and with no support or guarantee",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Van Johnson",
            "email": "eric@diegodev.com",
            "homepage": "https://twitter.com/shocm",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "webpatser/laravel-uuid": "^3.0",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ddg\\Lexted\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ddg\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ddg\\Lexted\\LextedServiceProvider"
            ]
        }
    }
}
